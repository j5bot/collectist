<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>collectist</application>
  <threadsafe>true</threadsafe>
  <version>2</version>
    <static-files>
        <include path="/**.gif" />
        <include path="/**.jpg" />
        <include path="/**.png" />
        <include path="/**.css" />
        <include path="/**.js" />
        <include path="/**.json" />
        <include path="/**.html" />

        <exclude path="/**.scss" />
    </static-files>

    <static-error-handlers>
        <handler file="index.html" />
    </static-error-handlers>
</appengine-web-app>
